{
  "name": "npm-templates-web-blue",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=development",
    "deploy": "jahia-deploy",
    "watch": "yarn build --env deploy=true --watch",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "jahia": {
    "module-type": "templatesSet",
    "server": "dist/main.js",
    "Jahia-Depends": "calendar,default,default-skins,event,facets,grid,news,profile,publication,search,sitemap,skins"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@jahia/eslint-config": "^2.1.2",
    "@jahia/scripts": "^1.3.3",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "extra-watch-webpack-plugin": "^1.0.3",
    "handlebars-loader": "^1.7.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "handlebars-helper-repeat": "^2.0.0"
  },
  "maven": {
    "groupId": "org.jahia.modules.npm",
    "distributionManagement": {
      "repository": {
        "id": "jahia-releases",
        "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-releases"
      },
      "snapshotRepository": {
        "id": "jahia-snapshots",
        "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-snapshots"
      }
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=3.0.0"
  },
  "packageManager": "yarn@3.6.1"
}
