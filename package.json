{
  "name": "templates-web-blue",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=development",
    "pwd": "dotenv -- echo $HOST",
    "doDeploy": "curl -s --user ${JAHIA_USER} --form bundle=@./templates-web-blue-v1.0.0.tgz --form start=true ${JAHIA_HOST}/modules/api/bundles",
    "deploy": "dotenv yarn doDeploy",
    "watch": "yarn build --env deploy=true --watch"
  },
  "jahia": {
    "module-type": "templatesSet",
    "server": "dist/main.js",
    "Jahia-Depends": "calendar,default,default-skins,event,facets,grid,news,profile,publication,search,sitemap,skins"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "handlebars-loader": "^1.7.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "handlebars": "^4.7.6"
  }
}
