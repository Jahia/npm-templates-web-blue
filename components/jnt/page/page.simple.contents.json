{
  "illustration": {
    "name": "illustration",
    "nodeType": "jnt:absoluteArea",
    "properties":{
      "j:level": "1",
      "j:allowedTypes": ["jnt:imageReference"]
    }
  },
  "pagecontent": {
    "main": {
      "breadcrumb": {
        "name": "pageBreadcrumb",
        "nodeType": "jnt:pageBreadcrumb",
        "properties": {
          "displayHome": "true",
          "displayCurrentPage": "true",
          "displayLinkOnCurrentPage": "false",
          "displayOnFirstLevel": "false"
        }
      }
    },
    "aside": {
      "sideMenu": {
        "name": "navMenu",
        "nodeType": "jnt:navMenu",
        "properties": {
          "j:baselineNode": "home",
          "j:maxDepth": "3",
          "j:startLevel": "1",
          "j:menuItemView":"menuElement",
          "j:styleName": "navigationN2-2"
        }
      }
    }
  }
}